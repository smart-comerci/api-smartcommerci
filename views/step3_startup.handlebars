.checkSmart {
    display: block;
    position: relative;
    padding-left: 35px;
   margin: 20% 25% !important;
    border-radius: 2px;
    cursor: initial;
    font-size: 14px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
  }
  

  .checkSmart input {
    position: absolute;
    opacity: 0;
    cursor: pointer;
    height: 0;
    width: 0;
  }

  

  .subSmart input {
    position: absolute;
    opacity: 0;
    cursor: pointer;
    height: 0;
    width: 0;
  }
  .imgContainer{
      max-width: 70px !important;
  }
  

  .checkmark {
    position: absolute;
    top: 0;
    left: 0;
    height: 25px;
    width: 25px;
    background-color: white;
    border: 1px solid #687C97;
    border-radius: 30%;
  }
  
  
  .checkSmart:hover input ~ .checkmark {
    background-color: white;
  }
  

  .checkSmart input:checked ~ .checkmark {
    background-color: #50C682;
  }
  

  .checkmark:after {
    content: "";
    position: absolute;
    display: none;
  }
  
  
  .checkSmart input:checked ~ .checkmark:after {
    display: block;
  }
  

  .checkSmart .checkmark:after {
    left: 9px;
    top: 5px;
    width: 5px;
    height: 10px;
    border: solid white;
    border-width: 0 3px 3px 0;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
  }






  .subSmart:hover input ~ .checkmark {
    background-color: white;
  }
  

  .subSmart input:checked ~ .checkmark {
    background-color: #50C682;
  }
  

  .subSmart:after {
    content: "";
    position: absolute;
    display: none;
  }
  
  
  .subSmart input:checked ~ .checkmark:after {
    display: block;
  }
  

  .subSmart .checkmark:after {
    left: 9px;
    top: 5px;
    width: 5px;
    height: 10px;
    border: solid white;
    border-width: 0 3px 3px 0;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
  }

