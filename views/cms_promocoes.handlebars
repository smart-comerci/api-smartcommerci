                            <label style="font-size:10px !important; padding: 10px" class="infoLabel infoValor">${
                                cupomData.main.affiliates === 'all' || cupomData.main.allAffiliates  ? 'Todas' : cupomData.main.affiliates
                                }</label>
                        </div>
                        <div style="min-width: 150px !important; max-width: 150px !important;"
                            class="col-sm paddingTop20">
                            <label style="font-size:10px !important; padding: 10px"
                                class="infoLabel infoValor">${00}</label>
                        </div>
                        <div class="col-sm paddingTop20">
                            <label class="label" style="line-height: 1.5 !important;">${
                               cupomData.validate.withoutTimeLimits ? 'Ilimitado ...' : 
                               cupomData.validate.especificDate ? cupomData.validate.dateStart : 'Não especificado'
                                }</label>
