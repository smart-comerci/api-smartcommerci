<script>
    
    localStorage.idPageStartup = 3;
    localStorage.stage = 3;
    var mainHost = localStorage.MY_DOMAIN;
    localStorage.subStage = 0
    localStorage

    $("#razaoSocial").val(localStorage.razaoSocial);
    $("#cnpj").val(localStorage.CNPJ);

    $.ajax({
        type: "POST",
        url: mainHost + "/getAllAffiliates",
        data: { affiliate_id: localStorage.MASTER_ID},
        headers: {
            "x-access-token": localStorage.token,
            is_product_image: true,
        },
        success: function (data) {
            //console.log(data);
            localStorage.LOJAS_CADASTRADAS = JSON.stringify(data);
            for(const k in data){
                $("#lojes").append("<option>"+data[k].affiliates_business_name+"</option>")
                $("#cnpjtas").append("<option>"+data[k].affiliates_business_cnpj+"</option>")
                $("#telphones").append("<option>"+data[k].affiliates_business_telefone+"</option>")
                $("#enderecus").append("<option>"+data[k].affiliates_business_endereco+"</option>")
                
                $("#meils").append("<option>"+(data[k].affiliates_business_mail != null && data[k].affiliates_business_mail != 'null'? data[k].affiliates_business_mail :'' )+"</option>")
                
            }
        },
        error: function (data) {
            //console.log(data);
            if (data.responseJSON.message.indexOf("token") > -1) {
                alert("Necessário fazer login!<br>" + data.responseJSON.message);
                setTimeout(() => {
                    location.replace("/startup?login");
                }, 2000);
            } else {
                alert("Algo saiu errado!<br>" + data.responseJSON.message);
            }
        },
        complete: function () {
            // ao final da requisição...
        },
    });


    $(".checkSmart").change(function(){
        if($(this).attr("chec") == "checkado"){            
            $(this).removeAttr("chec")
             $(this).parent().parent().find(".mostra").fadeIn()
            $(this).parent().parent().find(".oculta").hide()
        }else{
            $(this).parent().parent().find(".mostra").hide()
            $(this).parent().parent().find(".oculta").fadeIn()
             $(this).attr("chec","checkado")
             
        }
    })
var horariosFull = {"segundaSexta":"Sem expediente","sabados":"Sem expediente", "domingos":"Sem expediente"}
    $(".continuarStep").click(function(){
        //console.log("....")
        switch(localStorage.subStage){
            case "0":
                if($("#cnpj").val() != "" && $("#razaoSocial").val() != ""){
                    localStorage.subStage = Number(localStorage.subStage) +1
                    $("#passo").hide()
                    $("#passo"+localStorage.subStage).fadeIn()
                   
                }else{
                    $("#cnpj").css("border", "1px solid red")
                    $("#razaoSocial").css("border", "1px solid red")
                    alert("Preencha todos os campos corretamente!")
                }
            break;
            case "1":
                if($("#email").val() != "" && $("#telefone").val() != ""){
                    $("#passo"+localStorage.subStage).hide()
                    localStorage.subStage = Number(localStorage.subStage) +1                
                    $("#passo"+localStorage.subStage).fadeIn()
               
                }else{
                    $("#email").css("border", "1px solid red")
                    $("#telefone").css("border", "1px solid red")
                    alert("Preencha todos os campos corretamente!")
                }

            break;
            case "2":
        
                $(".checkSmart").each(function(){
                    if($(this).attr("chec") == "checkado"){
                        if($(this).parent().parent().find(".inicio").attr("dia") == "segundaSexta" ){
                            horariosFull.segundaSexta = $(this).parent().parent().find(".inicio").val() +" às "+$(this).parent().parent().find(".fim").val()
                        }
                        if($(this).parent().parent().find(".inicio").attr("dia") == "domingos" ){
                            horariosFull.domingos = $(this).parent().parent().find(".inicio").val() +" às "+$(this).parent().parent().find(".fim").val()
                        }
                        if($(this).parent().parent().find(".inicio").attr("dia") == "sabados" ){
                            horariosFull.sabados = $(this).parent().parent().find(".inicio").val() +" às "+$(this).parent().parent().find(".fim").val()
                        }
                    }
                })
                localStorage.HORARIOS_CONF = JSON.stringify(horariosFull)
                $("#passo"+localStorage.subStage).hide()
                localStorage.subStage = Number(localStorage.subStage) +1                
                $("#passo"+localStorage.subStage).fadeIn()
                

            break;
            case "3":
            break;
            default:
            break;
        }
    })
</script>

